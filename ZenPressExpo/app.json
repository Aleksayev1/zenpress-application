{
  "expo": {
    "name": "ZenPress - Acupressão e TCM",
    "slug": "zenpress",
    "version": "1.0.0",
    "description": "Aplicativo de acupressão e medicina tradicional chinesa para alívio de dores e bem-estar",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zenpress.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.zenpress.app",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f7f10f8a"
      }
    },
    "privacy": "public",
    "primaryColor": "#10b981",
    "plugins": [
      "expo-build-properties"
    ]
  }
}
